#include <iostream>

int main() {
    int rows, cols;

    std::cout << "Enter the number of rows (6): ";
    std::cin >> rows;
    std::cout << "Enter the number of columns (6): ";
    std::cin >> cols;

    if (rows != 6 || cols != 6) {
        std::cout << "Please enter a 6x6 array size." << std::endl;
        return 1;
    }

    int array[6][6];
    
    std::cout << "Enter the elements of the 6x6 array:" << std::endl;
    for (int i = 0; i < 6; i++) {
        for (int j = 0; j < 6; j++) {
            std::cin >> array[i][j];
        }
    }

    int result[6][6];
    for (int i = 0; i < 6; i++) {
        for (int j = 0; j < 6; j++) {
            result[j][i] = array[i][j];
        }
    }
    
    std::cout << "Swapped 6x6 array:" << std::endl;
    for (int i = 0; i < 6; i++) {
        for (int j = 0; j < 6; j++) {
            std::cout << result[i][j] << " ";
        }
        std::cout << std::endl;
    }

    return 0;
}