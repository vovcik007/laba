#include <iostream>
#include <vector>

int main() {
    int n;
    std::cout << "Enter the number of elements you want in the sequence: ";
    std::cin >> n;

    if (n < 3) {
        std::cout << "The sequence requires at least 3 elements." << std::endl;
        return 1;
    }

    std::vector<int> sequence(n);
    sequence[0] = 2;
    sequence[1] = 4;
    sequence[2] = 6;

    for (int i = 3; i < n; i++) {
        sequence[i] = (sequence[i - 1] + sequence[i - 2]) * sequence[i - 3];
    }

    std::cout << "The resulting sequence is: ";
    for (int i = 0; i < n; i++) {
        std::cout << sequence[i] << " ";
    }

    std::cout << std::endl;
    return 0;
}